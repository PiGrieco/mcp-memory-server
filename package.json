{
  "name": "@mcp-memory/cli",
  "version": "1.0.0",
  "description": "Command-line interface for MCP Memory Server - AI agents with persistent memory",
  "main": "dist/index.js",
  "bin": {
    "mcp-memory": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/cli.js",
    "dev": "ts-node src/cli.ts",
    "test": "jest",
    "prepublishOnly": "npm run build",
    "setup": "node dist/setup.js",
    "setup-all": "node dist/setup.js --all",
    "setup-claude": "node dist/setup.js --tool=claude",
    "setup-gpt": "node dist/setup.js --tool=gpt",
    "setup-cursor": "node dist/setup.js --tool=cursor",
    "setup-lovable": "node dist/setup.js --tool=lovable",
    "setup-replit": "node dist/setup.js --tool=replit"
  },
  "keywords": [
    "ai",
    "memory",
    "mcp",
    "chatgpt",
    "claude",
    "cursor",
    "lovable",
    "replit",
    "automation",
    "machine-learning",
    "embedding",
    "vector-search",
    "smart-assistant"
  ],
  "author": {
    "name": "AiGot Srl",
    "email": "support@mcp-memory.ai",
    "url": "https://github.com/AiGotsrl"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/AiGotsrl/mcp-memory-server.git"
  },
  "bugs": {
    "url": "https://github.com/AiGotsrl/mcp-memory-server/issues"
  },
  "homepage": "https://github.com/AiGotsrl/mcp-memory-server#readme",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "commander": "^11.1.0",
    "inquirer": "^9.2.12",
    "chalk": "^5.3.0",
    "ora": "^7.0.1",
    "fs-extra": "^11.1.1",
    "node-fetch": "^3.3.2",
    "semver": "^7.5.4",
    "yaml": "^2.3.4",
    "dotenv": "^16.3.1",
    "axios": "^1.6.0"
  },
  "devDependencies": {
    "@types/node": "^20.8.10",
    "@types/inquirer": "^9.0.7",
    "@types/fs-extra": "^11.0.4",
    "typescript": "^5.2.2",
    "ts-node": "^10.9.1",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.8"
  },
  "files": [
    "dist/",
    "templates/",
    "scripts/",
    "README.md",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/AiGotsrl"
  }
} 